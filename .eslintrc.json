{
    "extends": "airbnb",
    "rules": {
        "no-bitwise": ["error", {"allow": ["~"]}],
        "class-methods-use-this": "off",
        "max-len": [2, 150],
        "indent": ["error", 4],
        "object-curly-spacing": ["error", "never"],
        "no-underscore-dangle": 0,
        "curly": [2, "multi"],
        "no-param-reassign": ["error", {"props": false}],
        "no-console": ["error", { "allow": ["log", "warn", "error"] }]
    }
}
